# Managed by salt stack
wpdn-deploy () { echo "Deploying ..."; sudo salt "$@*" state.sls code; }
alias wpdn-service-test='nc -vz '
alias wpdn-flavor="nova flavor-list"
alias wpdn-secgroup="nova secgroup-list"
alias wpdn-gentoken='openssl rand -base64 32'
alias besu='sudo -H bash -l'
alias scr='screen -dd -R'
